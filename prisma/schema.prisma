generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorReading {
  id          String   @id @default(cuid())
  pm25        Float?
  pm10        Float?
  temperature Float?
  humidity    Float?
  aqi         Int?
  createdAt   DateTime @default(now())
}
